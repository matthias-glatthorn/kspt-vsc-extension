{
  "name": "kspt-vsc-extension",
  "displayName": "kspt-vsc-extension",
  "description": "Key strokes per technology extension for vscode.",
  "author": "Matthias Glatthorn <mail@matthias-glatthorn.de> (https://matthias-glatthorn.de)",
  "repository": "https://github.com/theoreticalsampling/kspt-vsc-extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "npx jest",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "jest": "^29.4.3",
    "jest-cli": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
